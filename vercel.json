{
  "framework": null,
  "rewrites": [
    {
      "source": "/:username/:articleIndex/markdown",
      "destination": "/server.js"
    },
    {
      "source": "/:username/:articleIndex",
      "destination": "/public/index.html"
    },
    {
      "source": "/:username",
      "destination": "/public/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/public/index.html"
    }
  ]
}
